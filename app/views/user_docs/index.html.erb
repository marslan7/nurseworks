
<p id="notice" class="alert" role="alert"><%= notice %></p>

<div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-3 mb-md-0 text-gray-800">Documents</h1>
    <div class="d-flex">
        <%= link_to 'New Document', new_user_doc_path, class: "btn btn-primary mr-3" %>
        <% if current_user.role == "Super User" %>
            <%= link_to 'New Document Type', new_document_type_admin_users_path, id: "new_support_request_btn", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>

<%= render "index" %>
<%= paginate @user_docs, theme: 'twitter-bootstrap-4', pagination_class: "pagination-sm" %>